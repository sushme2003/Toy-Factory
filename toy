3. let num=prompt("enter numbers");
let arr=num.split(' ').map(Number);
let arr1=[],c=0;
find(arr,arr1,c);
function find(arr,arr1,c){
for(let i=0;i<arr.length;i++){
    if(arr[i]!=0)
    arr1.push(arr[i]);
    else
    c++;
}
for(let i=0;i<c-1;i++){
    arr1.push(0);
}
console.log(arr1);
console.log(c);
}
